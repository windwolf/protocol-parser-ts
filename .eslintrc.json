{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/no-non-null-assertion": 1,
        "no-bitwise": 1
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}